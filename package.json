  "config": {
    "cache": {
      "path": "cache"
    },
    "download": {
      "useProxy": true
    },
    "express": {
      "port": 3000
    },
    "sync": {
      "IDLength": 6,
      "storeTimeoutMS": 86400000
    },
    "workers": {
      "maxCount": 10
    }
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-validator": "^6.14.3",
    "file-system-cache": "^2.0.2",
    "moment": "^2.29.4",
    "youtube-video-exists": "^0.1.2",
    "youtubei.js": "^2.9.0"
  },
  "description": "",
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/mocha": "^10.0.1",
    "chai": "^4.3.7",
    "chai-http": "^4.3.0",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "license": "MIT",
  "main": "bundle/src/index.js",
  "mocha": {
    "extensions": [
      "ts",
      "tsx"
    ],
    "loader": "ts-node/esm",
    "spec": [
      "tests/**/*.spec.*"
    ],
    "watch-files": [
      "src"
    ]
  },
  "name": "@musicly/lib",
  "private": false,
  "repository": {
    "type": "git",
    "url": "github:KT-Trez/musicly_lib"
  },
  "scripts": {
    "build": "tsc",
    "build-watch": "tsc -w",
    "start": "node bundle/src/index.js",
    "start-watch": "nodemon",
    "test": "mocha --config package.json --check-leaks --no-warnings"
  },
  "types": "bundle/src/index.d.ts",
  "version": "3.1.0"
}
